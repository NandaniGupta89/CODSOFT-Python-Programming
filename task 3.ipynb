   "source": [
    "import random\n",
    "\n",
    "import string\n",
    "\n",
    "len = int(input(\"Enter Length: \"))\n",
    "\n",
    "chars = string.ascii_letters\n",
    "\n",
    "chars += string.digits\n",
    "\n",
    "chars += string.punctuation\n",
    "\n",
    "password = \"\"\n",
    "\n",
    "for i in range(len):\n",
    "    password += random.choice(chars)\n",
    "print(\"Generated password:\", password)"
   ]
